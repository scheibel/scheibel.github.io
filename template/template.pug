block variables
  - var PageName = "#{context.name}"
  - var PageID = "index"

doctype html
html(lang="en")
  include /partials/head

  body#page-top(data-spy = 'scroll')
    section#navbar
      block navbar

    section.container#content
      .row
        .col-12.col-md-8
          block content
        .col-12.col-md-4 
          block sidecontent

    section#footer.container
      include /partials/footer

  include /partials/scripts
